{
  "name": "crayons",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freshdesk/crayons.git"
  },
  "keywords": [
    "web-components",
    "stenciljs",
    "ui-kit",
    "typescript",
    "components"
  ],
  "author": "Freshworks Inc",
  "version": "0.0.1",
  "description": "Crayons for Developer Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/crayons/crayons.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:components": "stencil build --docs",
    "build": "yarn build:components && yarn storybook:build && yarn docs:build",
    "code-sanity": "yarn lint",
    "dev": "npm-run-all --parallel start storybook",
    "docs:build": "vuepress build src",
    "docs:dev": "yarn build:components && vuepress dev src",
    "generate": "plop",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t grouped -s node_modules/custom-tslint-formatters/formatters",
    "lint": "yarn lint:ts && yarn lint:styles",
    "start": "stencil build --docs --dev --watch",
    "storybook:build": "build-storybook -c .storybook -o storybook-dist",
    "storybook:test": "npm run test:generate && npm run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s www",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "test": "stencil test --spec --e2e",
    "unit-tests": "npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@stencil/core": "1.9.2",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.1",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-events": "^5.3.14",
    "@storybook/addon-jest": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/html": "^5.3.14",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/web-components": "^5.3.14",
    "@types/autoprefixer": "^9.6.1",
    "@types/jest": "^24.0.23",
    "@types/node": "12.12.22",
    "@types/puppeteer": "^1.19.1",
    "@vue/babel-preset-app": "^4.1.0",
    "@vuepress/plugin-active-header-links": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.1",
    "custom-tslint-formatters": "^2.4.0",
    "html-to-react": "^1.4.2",
    "jest": "^24.8.0",
    "jest-cli": "24.9.0",
    "lit-html": "^1.1.2",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.4.0",
    "puppeteer": "2.1.0",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-ionic-rules": "^0.0.21",
    "typescript": "^3.6.4",
    "vuepress": "^1.2.0",
    "vuepress-plugin-live": "^1.4.2",
    "web-component-analyzer": "^1.0.0",
    "write-file-webpack-plugin": "^4.5.1",
    "yarn": "^1.22.4"
  },
  "license": "MIT",
  "dependencies": {
    "stencil": "^0.0.5"
  }
}
