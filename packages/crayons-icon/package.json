{
  "name": "@freshworks/crayons-icon",
  "repository": {
    "type": "git",
    "url": "https://github.com/freshworks/crayons.git"
  },
  "version": "3.0.0-beta.8",
  "description": "Crayons-Icon as NodeJS-CLI Tool to optimize SVGs and generate JS Imports for icons.",
  "main": "dist/index.js",
  "bin": {
    "fw-Optimize-Icons": "./bin/index.cli.js"
  },
  "keywords": [
    "crayons-icon",
    "Freshworks",
    "crayons-icon-cli"
  ],
  "files": [
    "dist/",
    "icons/",
    "utils/",
    "bin/"
  ],
  "scripts": {
    "test": "",
    "build": "npm run optimize-icons && gulp",
    "generateSysIcons": "node ./build.icons.system",
    "optimize-icons": "node ./bin/index.cli.js",
    "format": "prettier --write \"./**/*.{js,json}\""
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Freshworks Inc",
  "dependencies": {
    "chalk": "^5.0.0",
    "cli-alerts": "^1.2.2",
    "cli-handle-error": "^4.4.0",
    "cli-handle-unhandled": "^1.1.1",
    "cli-meow-help": "^2.0.2",
    "cli-welcome": "^2.2.2",
    "meow": "^9.0.0",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-svgmin": "^4.1.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "svgmin": "^0.1.0"
  }
}
